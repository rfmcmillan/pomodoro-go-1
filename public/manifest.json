{
  "name": "PomodoroGo",
  "description": "PomodoroGo is an app that helps boost productivity by blocking social media and providing a focus timer and data dashboard.",
  "version": "1.0",
  "manifest_version": 3,
  "chrome_url_overrides": {
  },
  "icons": {
    "128": "logo-pomo.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "alarms",
    "notifications",
    "unlimitedStorage"
  ],
  "content_security_policy": {},
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "PomodoroGo",
    "default_icon": "logo-pomo.png"
  },
  "externally_connectable": {
    "matches": ["*://pomodoro-go-1.herokuapp.com/*", "*://localhost/*"]
  },
  "oauth2": {
    "client_id": "811227993938-bdnup0onkbhf33rejoirhia20j2kf51r.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/userinfo.email"]
  }
}

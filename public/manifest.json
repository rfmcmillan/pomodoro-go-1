{
  "name": "PomodoroGo",
  "description": "Inspired by the Pomodoro technique, PomodoroGo is an app that helps boost productivity by blocking social media, setting timers and creating focus sessions. Share what you've achieved with your friends, and make work a more interesting experience! ",
  "version": "1.0",
  "manifest_version": 3,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "128": "pngegg.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "alarms",
    "notifications",
    "unlimitedStorage",
    "scripting",
    "http://*/",
    "https://*/",
    "<URL>"
  ],
  "host_permissions": ["http://*/", "https://*/"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "externally_connectable": {
    "matches": ["*://pomodoro-go-1.herokuapp.com/*", "*://localhost/*"]
  },
  "oauth2": {
    "client_id": "811227993938-bdnup0onkbhf33rejoirhia20j2kf51r.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/userinfo.email"]
  },
  "key": ""
}
